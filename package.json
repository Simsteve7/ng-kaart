  "lint-staged":  {
    "linters": {
      "src/**/*.ts": [
        "tslint --type-check --project src/tsconfig.app.json --fix",
        "awk /apps-(dev|tei).mow.vlaanderen.be/{exit(1)}",
        "git add"
      ],
      "projects/ng-kaart/**/*.ts": [
        "tslint --type-check --project projects/ng-kaart/tsconfig.lib.json --fix",
        "awk /apps-(dev|tei).mow.vlaanderen.be/{exit(1)}",
        "git add"
      ]
    },
    "ignore": ["**/test.ts"]
  },
